// Write a program to check whether a given number is prime or not using loops.

#include <stdio.h>

int main()
{
   int num, start, inc;

   start = inc = 2;

   printf("Enter the number to check whether it is prime or not: ");
   scanf("%d", &num);

    while(inc < num)
    {
        start * inc;
        
        
        inc++;
    }


    return 0;
}